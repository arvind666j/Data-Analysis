--Average Time of Process per Machine
select machine_id,round(avg(end_time-start_time),3) as processing_time from (
select machine_id, process_id, sum(start_time) as start_time,sum(end_time) as end_time
 from  (
select machine_id,process_id,case when activity_type='start' then timestamp else 0 END as start_time, case when activity_type='end' then timestamp else 0 END as End_time  

from Activity) a group by machine_id,process_id ) b group by machine_id
